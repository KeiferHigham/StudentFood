datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Discount {
  id                 Int      @id @default(autoincrement())
  restaurantName     String
  restaurantAddress  String
  discount           String
}

model Submission {
  id                 Int      @id @default(autoincrement())
  restaurantName     String
  restaurantAddress  String
  discount           String
}

model NearbyRestaurants {
  id                Int      @id @default(autoincrement())
  restaurantName String
  restaurantAddress String
}

