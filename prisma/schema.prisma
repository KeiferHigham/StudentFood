datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}


model Submissions {
  id                Int      @id @default(autoincrement())
  restaurantName    String
  restaurantAddress String
  discount          String
  latitude          Float
  longitude         Float
  verified          Boolean  @default(false)
  createdAt         DateTime @default(now())
}

model NearbyRestaurants {
  id                Int      @id @default(autoincrement())
  restaurantName String
  restaurantAddress String
  @@unique([restaurantName, restaurantAddress])
}

