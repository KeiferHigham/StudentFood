datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}


model Submissions {
  id                Int      @id @default(autoincrement())
  restaurantName    String
  restaurantAddress String
  discount          String
  verified          Boolean  @default(false)
}

model NearbyRestaurants {
  id                Int      @id @default(autoincrement())
  restaurantName String
  restaurantAddress String
}

